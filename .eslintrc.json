{
	"env": {
		"browser": true,
		"es2021": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"extends": [
		"eslint:recommended"
		//"eslint-config-prettier"
		// "plugin:react/recommended",
		// "plugin:react/jsx-runtime",
		// "standard",
		// "airbnb",
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		// off, warn, error
		"no-var": "error",
		"react/prop-types": "off",
		"react/react-in-jsx-scope": "off"
	}
}